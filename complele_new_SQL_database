

DROP TABLE IF EXISTS `country`;

CREATE TABLE `country` (
  `COU_ID` int(11) NOT NULL,
  `COU_NAME` varchar(255) DEFAULT NULL,
  `COU_CONTINENT` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`COU_ID`)
);


